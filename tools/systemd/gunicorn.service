[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/geolomas-back
ExecStart=/home/ubuntu/.local/share/virtualenvs/geolomas-back-XVGhZdP0/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/geolomas-back/geolomas.sock geolomas.wsgi:application

[Install]
WantedBy=multi-user.target
